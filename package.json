{
  "name": "straal",
  "version": "0.0.1",
  "description": "Wrapper for Xray's REST API",
  "author": {
    "name": "JoÃ£o Galego",
    "email": "jgalego1990@gmail.com",
    "url": "http://jpsgalego.byethost11.com"
  },
  "files": [
    "lib"
  ],
